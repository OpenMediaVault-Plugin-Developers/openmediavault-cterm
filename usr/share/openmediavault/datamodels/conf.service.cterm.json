{
  "type": "config",
  "id": "conf.service.cterm",
  "title": "cterm",
  "queryinfo": {
    "xpath": "//services/cterm",
    "iterable": false
  },
  "properties": {
    "enable": {
      "type": "boolean"
    },
    "host": {
      "type": "string",
      "default": "0.0.0.0"
    },
    "port": {
      "type": "integer",
      "minimum": 1024,
      "maximum": 65535,
      "default": 5000
    },
    "hostshell": {
      "type": "boolean"
    },
    "autosecret": {
      "type": "string",
      "default": "",
      "oneOf": [
        {
          "type": "string",
          "minLength": 32,
          "maxLength": 64
        },
        {
          "type": "string",
          "maxLength": 0
        }
      ]
    },
    "autouser": {
      "type": "string",
      "default": ""
    },
    "basepath": {
      "type": "string",
      "default": ""
    }
  }
}
